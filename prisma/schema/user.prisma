model User {
  id        String     @id @default(uuid())
  email     String     @unique
  password  String
  role      Role       @default(EMPLOYEE)
  status    Status     @default(ACTIVE)
  createdAt DateTime   @default(now())
  updatedAt DateTime   @updatedAt
  admin    Admin?
  employee Employee?
  manager  Manager?
  @@map("users")
}
