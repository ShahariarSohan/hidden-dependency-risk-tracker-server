model Admin {
  id            String   @id @default(uuid())
  email     String     @unique
  user          User     @relation(fields: [email], references: [email])
  name          String
  assignedTasks Task[]   @relation("AssignedByAdmin")
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
  @@map("admins")
}
