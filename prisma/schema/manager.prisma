model Manager {
  id            String   @id @default(uuid())
  name          String
  email     String     @unique
  user          User     @relation(fields: [email], references: [email])
  assignedTasks Task[]   @relation("AssignedByManager")
  contactNumber String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
  @@map("managers")
}
